<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:a="http://schemas.android.com/apk/res/android"
	a:orientation="vertical" a:layout_width="fill_parent" a:layout_height="fill_parent">


	<TextView
		a:id="@+id/main.select_server_1"
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		id="@+id/textView"
		a:layout_width="match_parent"
		a:layout_height="wrap_content"
		a:layout_marginLeft="10dip"
		a:layout_marginTop="6dip"
		a:text="Please provide you gender, age and favorite genres of music"
		a:textAppearance="?android:attr/textAppearanceLarge" />

	<TextView
		a:id="@+id/main.select_server_2"
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		id="@+id/textView3"
		a:layout_width="match_parent"
		a:layout_height="wrap_content"
		a:layout_marginLeft="10dip" />

	<LinearLayout
		a:layout_width="match_parent"
		a:layout_height="match_parent"
		a:orientation="horizontal">

		<TextView
			a:id="@+id/main.select_server_1"
			a:id="@+id/textView2"
			id="@+id/textView"
			a:layout_width="wrap_content"
			a:layout_width="match_parent"
			a:layout_height="wrap_content"
			a:layout_height="wrap_content"
			a:layout_marginLeft="10dip"
			a:layout_marginTop="6dip"
			a:layout_weight="1"
			a:text="gender"
			a:textAppearance="@android:style/TextAppearance.Material.Title" />
	</LinearLayout>

	<LinearLayout
		a:layout_width="match_parent"
		a:layout_height="match_parent"
		a:orientation="horizontal"></LinearLayout>

	<LinearLayout
		a:layout_width="match_parent"
		a:layout_height="match_parent"
		a:orientation="horizontal">

	</LinearLayout>
</LinearLayout>

