<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:a="http://schemas.android.com/apk/res/android">
    <item a:id="@android:id/background">
        <shape>
            <gradient
                a:startColor="#ffe9e9e9"
                a:centerColor="#ffc6c6c6"
                a:centerY="0.75"
                a:endColor="#ffe9e9e9"
                a:angle="270"/>

        </shape>
    </item>
    <item a:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    a:startColor="#ffe9e9e9"
                    a:endColor="#ff2165ca"
                    a:angle="270" />
            </shape>
        </clip>
    </item>
</layer-list>